{
    "name": "krak/enum-normalizer",
    "description": "Symfony Normalizer/Denormalizer for myclabs/enum",
    "type": "library",
    "homepage": "https://github.com/krakphp/enum-normalizer",
    "keywords": [
        "enum",
        "enum-normalizer",
        "symfony"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "RJ Garcia",
            "email": "ragboyjr@icloud.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Krak\\EnumNormalizer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Krak\\EnumNormalizer\\": "tests"
        }
    },
    "require": {
        "symfony/serializer": "^4.4|^5.4|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    },
    "scripts": {
        "test": "phpunit --testdox --colors tests"
    }
}
